{
	"extends": [
		"@cybozu/eslint-config/presets/kintone-customize-es5"
	],
	"env": {
		"browser": true
	},
	//  "plugins": ["prettier"],
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"allowImportExportEverywhere": false,
		"codeFrame": true
	},
	"rules": {
		"no-irregular-whitespace": [
			"error",
			{
				"skipRegExps": true
			}
		],
		"no-underscore-dangle": [
			"off"
		],
		//    "prettier/prettier": [
		//      "error",
		//      {
		//        "singleQuote": true,
		//        "semi": false
		//      }
		//    ],
		"yoda": 0,
		"no-unused-vars": 1
	},
	"globals": {
		"$": false,
		"kintoneUtility": false,
		"google": false,
		"kintone": false
	}
}